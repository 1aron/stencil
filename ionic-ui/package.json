  "scripts": {
    "build": "npm run build.clean && npm run build.commonjs && npm run compile.components && npm run copy.package.json",
    "build.clean": "rimraf dist",
    "build.commonjs": "tsc --project src --module commonjs --noImplicitUseStrict --outDir dist/commonjs",
    "build.2015": "tsc --project src",
    "copy.package.json": "cp scripts/build/package.json dist",
    "compile.components": "node scripts/build/compile-components.js"
  },
  "devDependencies": {
    "rimraf": "2.5.4",
    "tslint": "4.3.1",
    "typescript": "2.2.0"
  }
}
