{
  "compilerOptions": {
    "alwaysStrict": true,
    "allowSyntheticDefaultImports": true,
    "allowUnreachableCode": false,
    "declaration": true,
    "experimentalDecorators": true,
    "forceConsistentCasingInFileNames": true,
    "noEmitHelpers": true,
    "importHelpers": true,
    "lib": [
      "dom",
      "es2017",
      "esnext.array"
    ],
    "module": "esnext",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "outDir": "../../dist/transpiled-server",
    "pretty": true,
    "rootDir": "../",
    "target": "es2015",
    "baseUrl": ".",
    "paths": {
      "@build-conditionals": [
        "../compiler/app-core/build-conditionals.ts"
      ],
      "@declarations": [
        "../declarations/index.ts"
      ],
      "@platform": [
        "../client/index.ts"
      ],
      "@runtime": [
        "../runtime/index.ts"
      ],
      "@sys": [
        "../sys/node/index.ts"
      ],
      "@utils": [
        "../utils/index.ts"
      ]
    }
  },
  "files": [
    "./index.ts"
  ]
}
