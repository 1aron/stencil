{
  "name": "ionic-web",
  "private": true,
  "version": "88.0.0",
  "license": "MIT",
  "main": "es2015/index.js",
  "scripts": {
    "clean": "rimraf ../../../dist/transpiled-web/",
    "build": "npm run transpile && node scripts/build.web.js",
    "start": "npm install && npm run build",
    "transpile": "npm run clean && tsc -p src/tsconfig.json",
    "transpile.watch": "npm run clean && tsc -p src/tsconfig.json --watch"
  },
  "devDependencies": {
    "rimraf": "2.6.1",
    "typescript": "2.2.1"
  }
}
