{
  "compilerOptions": {
    "alwaysStrict": true,
    "allowSyntheticDefaultImports": true,
    "allowUnreachableCode": false,
    "declaration": true,
    "experimentalDecorators": true,
    "forceConsistentCasingInFileNames": true,
    "lib": [
      "dom",
      "es2017"
    ],
    "module": "esnext",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "outDir": "../../../dist/transpiled-renderer-vdom",
    "pretty": true,
    "rootDir": "../../",
    "target": "es2017",
    "baseUrl": ".",
    "paths": {
      "@build-conditionals": [
        "../../compiler/app-core/build-conditionals.ts"
      ],
      "@declarations": [
        "../../declarations/index.ts"
      ],
      "@platform": [
        "../../client/index.ts"
      ],
      "@runtime": [
        "../../runtime/index.ts"
      ],
      "@utils": [
        "../../utils/index.ts"
      ]
    }
  },
  "include": [
    "../../../types/*.d.ts"
  ],
  "files": [
    "./index.ts"
  ]
}
